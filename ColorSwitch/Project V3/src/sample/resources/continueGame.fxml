            <Image url="@../Style/Images/home.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="281.0" fitWidth="281.0" layoutX="94.0" layoutY="102.0" onMouseClicked="#restartGame" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Style/Images/restart.png" />
         </image>
      </ImageView>
      <Label fx:id="totalStarCount" layoutX="426.0" layoutY="4.0" text="0" textFill="#f8f7f7">
         <font>
            <Font name="SansSerif Regular" size="71.0" />
         </font>
      </Label>
      <Label layoutX="422.0" layoutY="73.0" text="STAR" textFill="#f5efef">
         <font>
            <Font name="SansSerif Regular" size="18.0" />
         </font>
      </Label>
      <Label fx:id="notEnoughStar" layoutX="157.0" layoutY="48.0" text="Not Enough Star" textFill="#eb0909">
         <font>
            <Font name="SansSerif Regular" size="21.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
